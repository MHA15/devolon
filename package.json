  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "jest",
    "eject": "craco eject",
    "test:coverage": "npm run test --watchAll false --coverage",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --quiet --fix"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      ".+\\.tsx?$": "babel-jest",
      ".+\\.svg$": "jest-transform-stub"
